  },
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-jsx-dom-expressions": "^0.31.9",
    "chokidar": "^3.5.2",
    "c8": "^7.11.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "solid-dom-testing": "^0.0.3",
    "solid-register": "0.1.5",
    "solid-testing-library": "^0.2.1",
    "uvu": "^0.5.3",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-solid": "^2.2.1"
  },
  "dependencies": {
    "solid-styled-components":"^0.27.3",
    "solid-js": "^1.3.2"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "solid-register": {
    "compile": {
      "solid": {
        "engine": "solid",
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
